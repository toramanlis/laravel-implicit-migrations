{
    "name": "toramanlis/implicit-migrations",
    "description": "Adds a command to create migrations by Eloquent models",
    "type": "library",
    "require": {
        "laravel/framework": "~11.20"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Toramanlis\\ImplicitMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toramanlis\\Tests\\": "tests/",
            "Toramanlis\\Tests\\Data\\": "tests/_data/"
        }
    },
    "authors": [
        {
            "name": "Timucin Bahsi",
            "email": "timucinbahsi@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Toramanlis\\ImplicitMigrations\\Providers\\ImplicitMigrationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "laravel/sail": "^1.41",
        "squizlabs/php_codesniffer": "^3.11"
    }
}
