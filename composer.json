{
    "name": "toramanlis/implicit-migrations",
    "description": "This package is a tool that creates Laravel migration files by inspecting the application's models by the command php artisan implicit-migrations:generate. Even after you change the model classes, you can run the command to generate a migration with the necessary update operations.",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "migration",
        "dev"
    ],
    "type": "library",
    "require": {
        "laravel/framework": "~12"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Toramanlis\\ImplicitMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Toramanlis\\Tests\\": "tests/",
            "Toramanlis\\Tests\\Data\\": "tests/_data/"
        }
    },
    "authors": [
        {
            "name": "Timucin Bahsi",
            "email": "timucinbahsi@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Toramanlis\\ImplicitMigrations\\Providers\\ImplicitMigrationsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^10.1",
        "laravel/sail": "^1.41",
        "squizlabs/php_codesniffer": "^3.12"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/toramanlis"
        }
    ]
}
